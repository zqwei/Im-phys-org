<div class="ui container" id='datatab'>
  <div class="ui two item secondary pointing menu">
    <a class="active item" data-tab="data_submit"><h2>Submit a dataset</h2></a>
    <a class="item" data-tab="data_collection"><h2>Collection</h2></a>
  </div>
  <div class="ui bottom attached active tab text" data-tab="data_submit">
    {{> _data_submit}}
  </div>
  <div class="ui bottom attached tab text" data-tab="data_collection">
    {{> _data_collection}}
  </div>
</div>

<script type="text/javascript">
  $(document).ready(function() {
    $('#datatab .menu .item').tab({
      context: $("#datatab")
    });
  });
</script>
